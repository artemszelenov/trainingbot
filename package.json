    "svelte-check": "^3.8.5",
    "vite": "^5.3.5"
  },
  "dependencies": {
    "@fontsource/inter": "^5.0.20",
    "@telegram-apps/sdk": "^1.1.3",
    "gramio": "^0.0.46"
